{
  "name": "w3a-antivirus",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "moveBackgroundJs": "webpack --env BROWSER=chrome --mode development ./src/background/index.ts -o ./build",
    "moveScriptJs": "webpack --env BROWSER=chrome --mode development ./src/contentScript/script.js -o ./build/script",
    "moveBackgroundJs:firefox": "webpack --env BROWSER=firefox --mode development ./src/background/index.ts -o ./build",
    "moveScriptJs:firefox": "webpack --env BROWSER=firefox --mode development ./src/contentScript/script.js -o ./build/script",
    "moveBackgroundJs:prod": "webpack --env BROWSER=chrome --mode production ./src/background/index.ts -o ./build",
    "moveScriptJs:prod": "webpack --env BROWSER=chrome --mode production ./src/contentScript/script.js -o ./build/script",
    "moveBackgroundJs:prod:firefox": "webpack --env BROWSER=firefox --mode production ./src/background/index.ts -o ./build",
    "moveScriptJs:prod:firefox": "webpack --env BROWSER=firefox --mode production ./src/contentScript/script.js -o ./build/script",
    "moveJSFiles": "yarn moveBackgroundJs && yarn moveScriptJs",
    "moveJSFiles:firefox": "yarn moveBackgroundJs:firefox && yarn moveScriptJs:firefox",
    "moveJSFiles:prod": "yarn moveBackgroundJs:prod && yarn moveScriptJs:prod",
    "moveJSFiles:prod:firefox": "yarn moveBackgroundJs:prod:firefox && yarn moveScriptJs:prod:firefox",
    "dev": "env-cmd -f ./environments/.env craco start && yarn moveJSFiles",
    "watch": "env-cmd -f ./environments/.env node ./scripts/watch.js && yarn moveJSFiles && IS_WATCH=true",
    "watch-win": "env-cmd -f ./environments/.env node ./scripts/watch.js && yarn moveJSFiles && set \"IS_WATCH=true\"",
    "audit": "audit-ci --config .audit-ci.json",
    "build": "env-cmd -f ./environments/.env.chrome craco build && yarn moveJSFiles && yarn test",
    "build:ci": "env-cmd -f ./environments/.env.chrome craco build && yarn moveJSFiles",
    "build:firefox": "env-cmd -f ./environments/.env.firefox craco build && yarn moveJSFiles:firefox",
    "build:prod": "env-cmd -f ./environments/.env.prod craco build && yarn moveJSFiles:prod && yarn test",
    "build:firefox:prod": "env-cmd -f ./environments/.env.prod.firefox craco build && yarn moveJSFiles:prod:firefox",
    "type-check": "tsc",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "lint": "eslint --ext .tsx --ext .ts ./src/*/** --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .tsx --ext .ts --fix ./src/*/** --no-error-on-unmatched-pattern",
    "prepare": "husky install",
    "e2e": "jest --detectOpenHandles",
    "test": "yarn e2e"
  },
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@reactflow/core": "^11.3.1",
    "@sentry/browser": "^7.21.1",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webextension-polyfill": "^0.10.0",
    "axios": "^0.24.0",
    "babel-jest": "^29.3.1",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "dagre": "^0.8.5",
    "date-fns": "^2.29.3",
    "detect-browser": "^5.3.0",
    "dotenv-webpack": "^8.0.1",
    "fast-levenshtein": "^3.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "node-sass": "7.0.3",
    "pluralize": "^8.0.0",
    "puppeteer": "^19.5.0",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-page-visibility": "^6.4.0",
    "react-router-dom": "^6.1.1",
    "react-scripts": "5.0.1",
    "reactflow": "^11.1.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "swr": "^1.1.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.2",
    "uuid": "^9.0.0",
    "web-vitals": "^1.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.210",
    "@types/dagre": "^0.7.48",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react-page-visibility": "^6.4.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "audit-ci": "5",
    "copy-webpack-plugin": "^11.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "webpack-cli": "^4.9.1",
    "webpack-extension-reloader": "^1.1.4"
  },
  "engines": {
    "node": "^16.10.0"
  },
  "browser": {
    "child_process": false
  },
  "resolutions": {
    "node-forge": ">=1.3.0",
    "glob-parent": ">=5.1.2",
    "tar": ">=4.4.18",
    "immer": ">=9.0.6",
    "ansi-html": ">=0.0.8",
    "url-parse": ">=1.5.8",
    "minimist": ">=1.2.6",
    "follow-redirects": ">=1.14.7",
    "async": ">=2.6.4",
    "ejs": ">=3.1.7",
    "nth-check": ">=2.0.1",
    "shell-quote": ">=1.7.3",
    "eventsource": ">=1.1.1",
    "scss-tokenizer": ">=0.4.3",
    "json5": ">=2.2.2",
    "webpack": ">=5.76.0",
    "http-cache-semantics": ">=4.1.1",
    "ua-parser-js": ">=0.7.33"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{tsx,ts}": "yarn lint:fix",
    "*.{scss}": "yarn lint:scss:fix"
  }
}
